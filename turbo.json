{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ],
      "env": [
        "BROKER_CREDENTIAL_ENCRYPTION_KEY"
      ],
      "outputLogs": "new-only"
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "dependsOn": [
        "^test"
      ],
      "outputs": [
        "coverage/**"
      ],
      "env": [
        "BROKER_CREDENTIAL_ENCRYPTION_KEY"
      ]
    },
    "test:cov": {
      "outputs": [
        "coverage/**"
      ],
      "env": [
        "BROKER_CREDENTIAL_ENCRYPTION_KEY"
      ]
    },
    "gql:schema": {
      "outputs": [
        "generated.graphql"
      ],
      "outputLogs": "new-only"
    },
    "gql:codegen": {
      "outputs": [
        "src/graphql/__generated__/**"
      ],
      "outputLogs": "new-only"
    },
    "prisma:generate": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "prisma:migrate": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "prisma:deploy": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "prisma:seed": {
      "cache": false,
      "outputLogs": "new-only"
    },
    "prisma:studio": {
      "cache": false,
      "persistent": true,
      "outputLogs": "new-only"
    }
  }
}
